{
  "version": 2,
  "builds": [
    {
      "src": "./back-end/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "./back-end/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "./index.html"
    }
  ],
  "env": {
    "MY_ENV_VAR": "my-secret-value"
  }
}
